{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "Triton.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames" : true,
      "js": [ "login_Tritonlink.js" ],
      "matches": [ "*://a4.ucsd.edu/tritON/Authn/*" ]
   }, {
	  "all_frames" : true,
      "js": [ "schedule.js" ],
      "matches": [ "*://act.ucsd.edu/myTritonlink20/display.htm" ]
   }],
   "description": "Chrome Extension for UCSD",
   "icons": {
      "128": "Triton.png",
      "16": "Triton.png",
      "48": "Triton.png"
   },
   "manifest_version": 2,
   "name": "TritonAssistant",
   "permissions": [ "storage" ],
   "version": "1.0"
}
