{
   "background": {
	  "page": "background.html"
   },
   "browser_action": {
      "default_icon": "icon48.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames" : true,
      "js": [ "login_Tritonlink.js" ],
      "matches": [ "*://a4.ucsd.edu/tritON/Authn/*" ]
   }, {
	  "all_frames" : true,
      "js": [ "schedule.js","jquery-1.8.3.min.js" ],
      "matches": [ "*://act.ucsd.edu/myTritonlink20/display.htm" ]
   }],
   "description": "Chrome Extension for UCSD",
   "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
   },
   "manifest_version": 2,
   "name": "TritonAssistant",
   "permissions": [ "storage" ],
   "version": "1.0"
}
